<ResourceDictionary x:Class="UnitTestBoilerplate.View.XamlResources"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UnitTestBoilerplate.View">
	<BooleanToVisibilityConverter x:Key="VisibilityConverter" />
	<Style x:Key="LineSection" TargetType="StackPanel">
		<Setter Property="Margin" Value="0 10 0 0" />
	</Style>
	<Style x:Key="LineTextBox" TargetType="TextBox">
		<Setter Property="Margin" Value="0 2 0 0" />
	</Style>
</ResourceDictionary>
